{
  "name": "mscgenjs-cli",
  "version": "1.12.5",
  "description": "Command line interface to mscgenjs",
  "main": "src/mscgen.js",
  "preferGlobal": true,
  "bin": {
    "mscgen_js": "bin/mscgen_js",
    "mscgenjs": "bin/mscgen_js",
    "mscgen.js": "bin/mscgen_js"
  },
  "dependencies": {
    "amdefine": "1.0.1",
    "commander": "2.11.0",
    "mscgenjs": "1.12.5",
    "phantomjs-prebuilt": "2.1.16",
    "requirejs": "2.3.5"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "chai-xml": "0.3.1",
    "eslint": "4.11.0",
    "istanbul": "0.4.5",
    "js-makedepend": "2.4.0",
    "mocha": "4.0.1",
    "npm-check-updates": "2.13.0",
    "nsp": "3.1.0"
  },
  "scripts": {
    "lint": "eslint src test",
    "lint:fix": "eslint --fix src test",
    "npm-check-updates": "ncu --upgrade",
    "nsp": "nsp check --warn-only",
    "postversion": "git push && git push --tags && npm publish",
    "preversion": "test `git branch | grep \"^* [a-zA-Z]\" | cut -c 3-` = 'master'",
    "test": "mocha -R dot --timeout 10000 --recursive test",
    "test:cover": "istanbul cover node_modules/mocha/bin/_mocha -- --timeout 12000 --recursive test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mscgenjs/mscgenjs-cli"
  },
  "author": "Sander Verweij",
  "contributors": [
    {
      "name": "djMax (Max Metral)",
      "url": "https://github.com/djMax"
    }
  ],
  "license": "GPL-3.0",
  "keywords": [
    "mscgen",
    "sequence chart",
    "sequence diagram",
    "xu",
    "msgenny"
  ],
  "engines": {
    "node": ">=4.0"
  },
  "os": [
    "!win32"
  ],
  "bugs": {
    "url": "https://github.com/mscgenjs/mscgenjs-cli/issues"
  },
  "homepage": "https://github.com/mscgenjs/mscgenjs-cli"
}
